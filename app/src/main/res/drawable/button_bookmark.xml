<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@drawable/ic_baseline_bookmark_24_on" android:state_selected="true"/>
    <!-- selector가 눌렸을때 @drawable/button01_press 를 보여준다.-->
    <item android:drawable="@drawable/ic_baseline_bookmark_24_on" android:state_pressed="true"/>

    <item android:drawable="@drawable/ic_baseline_bookmark_24_on" android:state_selected="false"/>
    <!-- selector가 눌리지 않았을때 @drawable/button01_normal 를 보여준다.-->
    <item android:drawable="@drawable/ic_baseline_bookmark_24_off" android:state_pressed="false"/>

</selector>